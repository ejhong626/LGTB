<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eddie.letsgetthisbread">

    <application
        android:allowBackup="true"
        android:icon="@drawable/bread_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/bread_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".StartScreen"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"
                  android:screenOrientation="portrait"/>
        <activity android:name=".ResultScreen"
                  android:screenOrientation="portrait"/>
        <activity android:name=".SettingsScreen"
                  android:screenOrientation="portrait"></activity>
        <activity android:name=".CharacterScreen"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>